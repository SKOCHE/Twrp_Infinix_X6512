# Fstab Recovery
system           /system         ext4    ro     wait,slotselect,logical
system_ext           /system_ext         ext4    ro     wait,slotselect,logical
vendor           /vendor         ext4    ro     wait,slotselect,logical
product          /product        ext4    ro     wait,slotselect,logical

/dev/block/platform/bootdevice/by-name/md_udc /metadata ext4 noatime,nosuid,nodev,discard wait,formattable,first_stage_mount

/dev/block/platform/bootdevice/by-name/data f2fs noatime,nosuid,nodev,discard,noflush_merge,fsync_mode=nobarrier,reserve_root=134217,resgid=1065,inlinecrypt wait,check,formattable,quota,latemount,resize,reservedsize=128m,checkpoint=fs,encryptable=footer

/dev/block/platform/bootdevice/by-name/misc      /misc     emmc defaults defaults
/dev/block/platform/bootdevice/by-name/boot /boot emmc defaults first_stage_mount,nofail,slotselect
